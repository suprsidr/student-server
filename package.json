{
  "name": "kit",
  "version": "1.0.0",
  "description": "Starting point for the Apollo GraphQL Server tutorial.",
  "main": "server.js",
  "dependencies": {
    "apollo-server-express": "^2.14.2",
    "body-parser": "^1.9.0",
    "casual": "^1.5.19",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
